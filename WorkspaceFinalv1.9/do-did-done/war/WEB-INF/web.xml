<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    <servlet>
		<servlet-name>UserLogin</servlet-name>
		<servlet-class>com.sandip.dodiddone.user.UserLoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserRegister</servlet-name>
		<servlet-class>com.sandip.dodiddone.user.UserRegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserLogin</servlet-name>
		<url-pattern>/userlogin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserRegister</servlet-name>
		<url-pattern>/userRegister</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>base.html</welcome-file>
	</welcome-file-list>
</web-app>

